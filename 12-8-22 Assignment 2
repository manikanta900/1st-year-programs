#include <stdio.h>
    int main()
{
  int i,j,n;
  printf("enter n value");
  scanf("%d",&n);
   for(i=1;i<=5;i++)
    {
      for(j=1;j<=i;j++)
        {
        printf("*");
        }
        printf("\n");
    }
     for(i=1;i<=5;i++)
    {
        for(j=1;j<=n-i;j++)
        {
            printf("*");
        }
        printf("\n");
    
    }
}
